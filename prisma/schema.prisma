// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Measure {
  id          Int      @id @default(autoincrement())
  value       Float
  imageUrl    String
  uuid        String
  date        DateTime
  measureType String
}

model Customer {
  id Int @id @default(autoincrement())
}
